END
SOLUTION 2
pH 7 charge
-temp 30
-pressure 493
USE equilibrium_phases 1
USE reaction_temperature 1
USER_GRAPH
 -headings P=500.bar
 1 data 3.994e+01, 4.981e+01, 6.146e+01, 6.718e+01, 7.947e+01
 2 dim t(29)
 3 read t(1), t(2), t(3), t(4), t(5)
