PRINT; -reset false
USER_GRAPH
 -headings Gypsum
# -plot_csv_file gp_anh.dat
 -axis_titles "temperature / °C" "Solubility / (mol/kgw)"
 -axis_scale x_axis 30 170
 -axis_scale y_axis 1e-3 0.05 auto auto log
